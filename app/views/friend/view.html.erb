<h1>Найденные числа</h1>
<table>
  <thead>
    <tr>
      <th>Первое число</th>
      <th>Второе число</th>
    </tr>
  </thead>
  <tbody>
    <% @result.each do |x| %>
      <tr>
        <% x.each do |number| %>
          <td><%= number %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "Повторить вычисления", :friend_input %>
